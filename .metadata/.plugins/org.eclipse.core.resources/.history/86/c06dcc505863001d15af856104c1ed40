package t1;

public class VectorConst extends Vector{
	private Vector vec;
	
	public VectorConst(Vector vec) {
		this.vec = vec;
	}

	@Override
	Vector2Df toFloat() {
		return vec.toFloat();
	}

	@Override
	Vector2Di toInt() {
		return vec.toInt();
	}

	@Override
	Vector add(Vector other) {
		return vec.add(other);
	}

	@Override
	Vector sub(Vector other) {
		return vec.sub(other);
	}

	@Override
	Vector mult(Vector other) {
		return vec.mult(other);
	}

	@Override
	Vector divd(Vector other) {
		return vec.divd(other);
	}

	@Override
	void scal(float s) {
		vec.scal(s);
	}

	@Override
	float abs() {
		return vec.abs();
	}
}

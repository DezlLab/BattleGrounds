package t1;

public abstract class Vector {
	
	private const VectorConst zero = new VectorConst(new Vector2Df(0, 0));
	private const VectorConst up;
	private const VectorConst down;
	private const VectorConst left;
	private const VectorConst right;
	
	public static Vector zero() {
		return zero;
	}
	
	public static Vector up() {
		return up;
	}
	
	public static Vector down() {
		return down;
	}
	
	public static Vector left() {
		return left;
	}
	
	public static Vector right () {
		return right;
	}
}
